datasource DS {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

model Post {
	id			Int @id @default(autoincrement())
	title		String
	body		String
	image		String
	createdAt	DateTime @default(now())
}

model Contact {
	id			Int @id @default(autoincrement())
	email		String
	createdAt	DateTime @default(now())
}